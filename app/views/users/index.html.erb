<%- content_for :body do -%>
<div class="span8 offset4">
	<% provide(:title, 'All users') %>

	<ul class="users">
		<h1>All users</h1>
		<% @users.each do |user| %>
		   <%= render partial: 'users/users_list', :locals => { user_list_element: user }  %>
		<% end %>
	</ul>
	<%= paginate @users %>
</div>
<%- end -%>

